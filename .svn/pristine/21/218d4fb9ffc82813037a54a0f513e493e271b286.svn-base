                <tr align="left" class="panel-header">
                    <th class=' com_TextCenter com_w10pen'>配置名称
                    </th>
                    <th class="com_TextCenter com_w25pen">配置描述
                    </th>
                    <th class='com_TextCenter com_w12pen'>操作
                    </th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
    <div id="dd"></div>
    <div id="loading"></div>
