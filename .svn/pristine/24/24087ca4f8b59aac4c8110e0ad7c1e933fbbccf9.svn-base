                        <tr>
                            <th class="com_w15pen">
                                压力名称
                            </th>
                            <th>
                               压力描述
                            </th>
                            <th class="com_w15pen">
                               状态值
                            </th>
                            <th class="com_w20pen">
                               操作
                            </th>
                        </tr>
                    </thead>
                <tbody></tbody>
            </table>
        </div>
        <div id="dd"></div>
        <div id="loading"></div>
    </body>
