<!DOCTYPE html>
<html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>用户信息</title>
        <link href="~/Scripts/JqEasyUI/themes/default/easyui.css" rel="stylesheet" />
        <link href="~/Style/Site.css" rel="stylesheet" />
        <link href="~/Style/common.css" rel="stylesheet" />
        <link href="~/Scripts/JqEasyUI/themes/icon.css" rel="stylesheet" />
        <script src="~/Scripts/jquery-1.7.1.min.js"></script>
        <script src="~/Scripts/Common/GlobalLoading.js"></script>
        <script src="~/Scripts/JqEasyUI/jquery.easyui.min.js"></script>
        <script src="~/Scripts/JqEasyUI/easyloader.js"></script>
        <script src="~/Scripts/Common/Common.js"></script>
        <script src="~/Scripts/Common/ui-com-table.js" type="text/javascript"></script>
        <script type="text/javascript">
            window.onload = function () {
                new comTab().fnInit();
            }
        </script>
    </head>
    <body>
        <div class=" com_TextCenter com_font13Px com_borderCom" style='height: 26px;'>
            所属公司:<select class="com_w15pen" id="UserCom" onchange="Common.GetCity()"></select>
            用户分组:<select class="com_w15pen" id="UserGroup"></select>
            真实姓名:<input type="text" class="com_w15pen" id="TrueName" />
            <input type="button" value="查询" class="buttonVer2" onclick="Common.SerarchCompany()" />
            <input type="button" class="buttonVer2" value="添加" onclick="Common.WindowBox('公司详细', 0.8, 0.8, 'ClientInfo/ClientDetail/Add/-1')" />
        </div>
        <div id="tbDiv" class="com_fl   com_font13Px" style="overflow-y: auto; overflow-x: hidden">
            <table id="ComList" class="ui-com-tab" rules="all">
                <thead>
                        <tr align="left" class="panel-header">
                            <th class="com_TextCenter com_w35pen">
                                公司名称
                            </th>
                            <th class=' com_TextCenter com_w10pen'>
                                联系人
                            </th>
                            <th class='com_TextCenter com_w10pen'>
                                联系电话
                            </th>
                            <th class='com_TextCenter com_w12pen'>
                                网站主页
                            </th>
                            <th class='com_TextCenter com_w12pen'>
                                公司邮箱
                            </th>
                            <th class='com_TextCenter'>
                                操作
                            </th>
                            </tr>
                    </thead>
                <tbody></tbody>
            </table>

        </div>
        <div id="detailWindow" class="easyui-window" title="My Window" modal="true" iconcls="icon-save" style="width: 500px; height: 200px; padding: 5px;">
            <div id="dd"></div>
            <div id="loading"></div>
    </body>

</html>

