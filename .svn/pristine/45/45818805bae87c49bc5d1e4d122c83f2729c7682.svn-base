                type: "POST",
                data: SiteObj,
                url: "/SiteInfo/UpdateSite",
                error: function () { alert("请求失败，请稍后再试"); },
                success: function (data) {
                    if (data != "ok") {
                        alert(data);
                    }
                    else {
                        alert("修改成功");
                    }
                }
            });
        }
    },

