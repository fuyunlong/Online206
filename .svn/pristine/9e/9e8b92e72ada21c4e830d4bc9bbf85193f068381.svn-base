var Config = {
    //搜索配置信息
    SerarchConfig: function () {
        var configName = $("#txtConfigName").val().trim() || "";
        $.ajax({
            type: "POST",
            data: "configName=" + configName,
            url: "/Config/GetConfigByName",
            dataType: "json",
            error: function (xhr) {
                Common.MsgBox("查询配置信息失败,请稍后再试");
            },
            success: function (data) {
                var Str = "";
                for (var i = 0; i < data.length; i++) {
                    Str += "<tr><td class='com_w30pen'><input type='hidden' value=" + data[i].ConfigId + ">" + data[i].ConfigName + "</td>";
                    Str += "<td class='com_w25pen'>" + data[i].IP + "</td>";
                    Str += "<td class='com_w20pen'>" + data[i].Port + "</td>";
                    Str += "<td class='com_w25pen'>";
                    Str += "<a href='javascript:void(0)' onclick=Common.WindowBox('配置查看',0.8,0.8,'Config/ShowOper/Show/" + data[i].ConfigId + "')>查看|</a>";
                    Str += "<a href='javascript:void(0)' onclick=Common.WindowBox('配置修改',0.8,0.8,'Config/ShowOper/Edit/" + data[i].ConfigId + "')>修改|</a>";
                    Str += "<a href='javascript:void(0)' onclick=Config.DeleteConfig(" + data[i].ConfigId + ")>删除</a>";
                    Str += "</tr>";
                }
                $("#ConfigList tbody").html(Str);
            }
        });
    }
};