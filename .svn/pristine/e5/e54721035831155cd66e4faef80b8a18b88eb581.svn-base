<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>添加站点</title>
    <link href="~/Style/common.css" rel="stylesheet" />
    <link href="~/Style/Site.css" rel="stylesheet" />
    <link href="~/Style/ui-com-table.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.7.1.min.js"></script>
        <script src="~/Scripts/Common/GlobalLoading.js"></script>
    <script src="~/Scripts/My97DatePicker/WdatePicker.js"></script>
    <script src="~/Scripts/Common/Common.js"></script>
    <script src="~/Scripts/SiteMng/SiteMng.js"></script>
</head>
 <body onload="$('#loading').hide()">
    <div>
        <table class="ui-com-tabFrm">
            <tbody>
                <tr>
                    <td>所属公司
                    </td>
                    <td>
                        <select id="Company" onchange="Common.GetGroupsByComId(this.value)">        
                           <option value=@ViewBag.obj.CompanyId>@ViewBag.obj.CompanyName</option>
                        </select>
                    </td>
                    <td>站点分组</td>
                    <td>
                        <select id="Group">
                            <option value=@ViewBag.obj.GroupCode>@ViewBag.obj.GroupName</option>
                        </select></td>
                    <td>站点名称</td>
                    <td>
                        <input type="text" id="SiteName" /></td>
                        <select id="SiteName">
                            <option value="@ViewBag.obj.Dtuid">@ViewBag.obj.DtuName</option>
                        </select></td>
                </tr>
                <tr>
                    <td>阀门编号</td>
                    <td>
                        <input type="text" disabled="disabled" id="ValveCode"  value="@ViewBag.obj.ValveCode" /></td>
                    <td>阀门名称</td>
                    <td>
                        <input type="text" id="ValveName" onchange=""   value="@ViewBag.obj.ValveName"/></td>
                </tr>
                <tr>
                    <td>关阀时间</td>
                    <td>
                        <input type="text" id="ClosedTime" value="@ViewBag.obj.ClosedTime" />
                    </td>
                    <td>影响区域</td>
                    <td>
                        <input type="text" id="EffctArea"  value="@ViewBag.obj.EffctArea"/>
                    </td>
                    <td>影响户数</td>
                    <td>
                        <input type="text" id="EffctUserNum"  value="@ViewBag.obj.EffctUserNum"/>
                    </td>
                </tr>
               
            </tbody>
        </table>
    </div>
 <div id="loading" style="position: absolute; height: 100%; width: 100%; filter: alpha(opacity = 40); -moz-opacity: 0.4; opacity: 0.4; background: gray url(/Images/loading.gif) center center no-repeat; z-index: 100000; top: 0px; left: 0px"></div>
</body>
</html>
