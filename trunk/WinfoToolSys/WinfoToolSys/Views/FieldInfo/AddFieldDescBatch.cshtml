<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>批量添加配置页面</title>
    <link href="~/Scripts/JqEasyUI/themes/default/easyui.css" rel="stylesheet" />
    <link href="~/Style/Site.css" rel="stylesheet" />
    <link href="~/Style/common.css" rel="stylesheet" />
    <link href="~/Scripts/JqEasyUI/themes/icon.css" rel="stylesheet" />
    <script src="http://libs.baidu.com/jquery/1.8.3/jquery.min.js"></script>
    <script src="~/Scripts/layer/layer.min.js"></script>
    <script src="~/Scripts/Common/GlobalLoading.js"></script>
    <script src="~/Scripts/Common/Common.js"></script>
    <script src="~/Scripts/FieldMng/FiledMng.js"></script>
    <script src="~/Scripts/Common/ui-com-table.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(function () {
            objFieldInfoQ.fnLoad();
        });
        var objFieldInfoQ = {
            objTab: null,
            fnBind: function () {
                $("#filedComList").css("height", $("#divRight").css("height"));
                $("#tbDiv").css("height", $(window).height() * 0.87);
                $("#btnQuery").click(function () {
                    FiledMng.GetFiledList();
                    objFieldInfoQ.objTab.fnResizeTable();
                });
            },
            fnLoad: function () {
                objFieldInfoQ.fnBind();
                objFieldInfoQ.objTab = new comTab();
                objFieldInfoQ.objTab.fnInit();
            }
        };
    </script>
</head>
<body>
    <div id="tbDiv" class="com_fl com_font13Px" style="overflow-y: auto; overflow-x: hidden">
        <table id="ComList" class="ui-com-tab" rules="all">
            <thead>
                <tr align="left" class="panel-header">
                    <th class=' com_TextCenter'>编号
                    </th>
                    <th class='com_TextCenter'>名称
                    </th>
                    <th>短描述
                    </th>
                    <th>描述
                    </th>
                    <th class='com_TextCenter'>单位
                    </th>
                    <th class='com_TextRight'>最小值
                    </th>
                    <th class='com_TextRight'>最大值
                    </th>
                    <th class='com_TextRight'>低报警值
                    </th>
                    <th class='com_TextRight'>高报警值
                    </th>
                    <th class='com_TextRight'>最低报警值
                    </th>
                         <th class='com_TextRight'>最高报警值
                    </th>
                </tr>
            </thead>
            <tbody>
                @{ 
                    
                    foreach (var a in ViewBag.Fields as List<Proxy.ServiceWinToolRead.T_DTU_FieldDesc>)
                    {
                    <tr>
                        <td>@a.Dtuid</td>

                        <td>@a.FieldName</td>
                        <td>
                            <input type="text" class="com_w90pen" value=" @a.FieldDesc"></td>
                   
                        <td>
                            <input type="text" class="com_w90pen" value=" @a.FieldShortDesc"></td>
                  
                        <td>
                            <input type="text" class="com_w90pen" value=" @a.FieldUnit"></td>
                        <td>
                            <input type="text" class="com_w90pen"  value="@a.ValueMin" /></td>
                        <td>
                            <input type="text"class="com_w90pen"   value="@a.ValueMax" /></td>
                        <td>
                            <input type="text"class="com_w90pen"   value="@a.Lowlimit" /></td>
                        <td>
                            <input type="text"class="com_w90pen"   value="@a.Highlimit" /></td>
                        <td>
                            <input type="text"class="com_w90pen"   value="@a.Lololimit" /></td>
                        <td>
                            <input type="text" class="com_w90pen"  value="@a.Hihilimit" /></td>
                    </tr>
                    }
                }
            </tbody>
        </table>
        <div style="text-align: center">
            @Html.Raw(ViewBag.Oper)
        </div>
    </div>
</body>
</html>
