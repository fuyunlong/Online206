<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>客户信息</title>
    <link href="~/Scripts/JqEasyUI/themes/default/easyui.css" rel="stylesheet" />
    <link href="~/Style/Site.css" rel="stylesheet" />
    <link href="~/Style/common.css" rel="stylesheet" />
    <link href="~/Style/Stip.css" rel="stylesheet" />
    <script src="~/Scripts/Stip.js"></script>
    <link href="~/Style/ui-com-table.css" rel="stylesheet" />
    <link href="~/Scripts/JqEasyUI/themes/icon.css" rel="stylesheet" />
    <script src="http://libs.baidu.com/jquery/1.8.3/jquery.min.js"></script>
    <script src="~/Scripts/layer/layer.min.js"></script>
    <script src="~/Scripts/Common/GlobalLoading.js"></script>
    <script src="~/Scripts/Common/Common.js"></script>
    <script src="~/Scripts/ClientMng/ClientInfoMng.js"></script>
    <script src="~/Scripts/Common/ui_common.js"></script>
    <script src="~/Scripts/FieldMng/FiledMng.js"></script>
    <script src="~/Scripts/UserMng/UserDtu.js"></script>
    <script src="~/Scripts/SiteMng/SiteMng.js"></script>
    <script src="~/Scripts/Common/Validate.js"></script>

</head>
<body onload="$('#loading').hide()">
    <div class="com_DivBor">
        <table class="ui-com-tabFrm" cellspacing="0" cellpadding="0">
            <tr>
                <td>站点编号</td>
                <td class="com_w10pen">
                    <select id="Dtuid">
                        @foreach (var a in ViewBag.DtuList)
                        {
                            <option value="@a.Dtuid">@a.Dtuid</option>
                        }
                    </select>
                </td>
                <td>字段名称</td>
                <td colspan="2">
                    <input id="ss" class="easyui-numberspinner com_w50pen" min="1" value="1" max="100" required="true" />
                    <select id="FiledName">
                        <option>QB </option>
                        <option>Q </option>
                        <option>VB </option>
                        <option>V </option>
                        <option>P </option>
                        <option>T</option>
                        <option>AI </option>
                        <option>DI </option>
                        <option>AO </option>
                        <option>BO </option>
                        <option>TC </option>
                        <option>PC </option>
                        <option>VC </option>
                    </select>
                </td>
                <td>字段单位</td>
                <td>
                    <input type="text" class="com_w70pen" id="FiledUint" />
                </td>
                <td>开启采集</td>
                <td>是 @Html.RadioButton("IsCollect", 1, true)
                    否 @Html.RadioButton("IsCollect", 0)
                </td>
            </tr>
            <tr>
                <td>短描述</td>
                <td>
                    <input type="text" id="FieldShortDesc" />
                </td>
                <td>字段描述</td>
                <td colspan="2">
                    <input type="text" id="FieldDesc" class="com_w80pen" />
                </td>
                <td>是否显示</td>
                <td>是 @Html.RadioButton("IsShow", 1, true)
                    否 @Html.RadioButton("IsShow", 0)
                </td>
                <td>报警检查</td>
                <td>是 @Html.RadioButton("IsAlearm", 1, true)
                    否 @Html.RadioButton("IsAlearm", 0)
                </td>
            </tr>
            <tr>
                <td>最小值</td>
                <td>
                    <input type="text" onkeydown="Validate.funOnlyNumber(this);" value="0" class="com_w70pen" id="ValueMin" />
                </td>
                <td>最大值</td>
                <td colspan="2">
                    <input type="text" onkeydown="Validate.funOnlyNumber(this);" value="1000" class="com_w70pen" id="ValueMax" />
                </td>
                <td>进出口类型</td>
                <td>
                    <select id="InOutType">
                        <option value="0">不分进出</option>
                        <option value="1">进口</option>
                        <option value="2">出口</option>
                    </select>

                </td>
                <td>排序序号</td>
                <td>
                    <input type="text" id="OrderId" value="0" onkeydown="Validate.funOnlyNumber(this);" class="com_w70pen" />

                </td>
            </tr>
            <tr>
                <td>低报警值</td>
                <td>
                    <input type="text" class="com_w70pen" value="-1" id="Lowlimit" />
                </td>
                <td>高报警值</td>
                <td colspan="2">
                    <input type="text" class="com_w70pen" value="9999999999999" onkeydown="Validate.funOnlyNumber(this);" id="Highlimit" />
                </td>
                <td>最低报警值</td>
                <td>
                    <input type="text" class="com_w70pen" value="-1" id="Lololimit" />
                </td>
                <td>最高报警值</td>
                <td>
                    <input type="text" class="com_w70pen" value="9999999999999" id="Hihilimit" />
                </td>
                <td colspan="2"></td>
            </tr>
            <tr>
                <td>统计类型</td>
                <td class="com_TextCenter">
                    <select id="CountType">
                        <option>不统计</option>
                        <option>平均</option>
                        <option>累计</option>
                        <option>示数</option>
                    </select>
                </td>
            </tr>
            <tr>
                <td colspan="10" class="com_TextCenter" id="OperArea">
                    <input type="button" class="buttonVer2" value="添加" onclick="FiledMng.AddFiled()" />
                </td>
            </tr>
        </table>
    </div>
</body>
</html>
